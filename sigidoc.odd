                           <desc xml:lang="en" versionDate="2024-03-04">The creator of the digital
                              record.</desc>
                        </valItem>
                     </valList>
                  </attDef>
               </attList>
            </elementSpec>

            <!-- figDesc -->
            <!-- AS 2023-12-09; last modified: AS 2024-05-28 -->
