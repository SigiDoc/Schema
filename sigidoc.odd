            <!-- provenance -->
            <!-- AS 2023-11-24/ last modified: AS 2024-03-17 -->
            <elementSpec ident="provenance" module="msdescription" mode="change">
               <content>
                  <!-- major changes to provenance content -->
                  <alternate>
                     <sequence>
                        <elementRef key="placeName" maxOccurs="unbounded"/>
                        <sequence minOccurs="0">
                           <elementRef key="rs" minOccurs="0" maxOccurs="unbounded"/>
                           <elementRef key="p" minOccurs="0" maxOccurs="unbounded"/>
                        </sequence>
                     </sequence>
                     <sequence>
                        <elementRef key="rs" maxOccurs="unbounded"/>
                        <sequence minOccurs="0">
                           <elementRef key="placeName" minOccurs="0" maxOccurs="unbounded"/>
                           <elementRef key="p" minOccurs="0" maxOccurs="unbounded"/>
                        </sequence>
                     </sequence>
                     <sequence>
                        <elementRef key="p" maxOccurs="unbounded"/>
                        <sequence minOccurs="0">
                           <elementRef key="placeName" minOccurs="0" maxOccurs="unbounded"/>
                           <elementRef key="rs" minOccurs="0" maxOccurs="unbounded"/>
                        </sequence>
                     </sequence>
                  </alternate>
               </content>
               <constraintSpec ident="provenance" scheme="schematron" mode="add">
                  <!-- constraining values of provenance/@subtype according to the @type's values -->
                  <constraint>
                     <sch:pattern>
                        <sch:rule context="tei:provenance[@type='found']">
                           <sch:assert
                              test="@subtype = ('discovered','rediscovered','first-seen','first-recorded')"
                              >With provenance/@type='found', @subtype's values are
                              'discovered','rediscovered','first-seen','first-recorded'.</sch:assert>
                        </sch:rule>
                        <sch:rule context="tei:provenance[@type='observed']">
                           <sch:assert
                              test="@subtype = ('seen','recorded','identified','photographed','autopsied','squeeze-taken','rubbing-taken','ink-transfer-taken','reported','built-into','reappeared')"
                              > With provenance/@type='observed', @subtype's values are
                              'seen','recorded','identified','photographed','autopsied','squeeze-taken','rubbing-taken','ink-transfer-taken','reported','built-into','reappeared'.
                           </sch:assert>
                        </sch:rule>
                        <sch:rule context="tei:provenance[@type='not-observed']">
                           <sch:assert
                              test="@subtype = ('lost','stolen','destroyed','drawn','reported-lost','reported-stolen','reported-destroyed')"
                              >With provenance/@type='not-observed', @subtype's values are
                              'lost','stolen','destroyed','drawn','reported-lost','reported-stolen','reported-destroyed'.
                           </sch:assert>
                        </sch:rule>
                        <sch:rule context="tei:provenance[@type='transferred']">
                           <sch:assert
                              test="@subtype = ('moved','sold','given','loaned','auctioned','donated')"
                              >With provenance/@type='transferred', @subtype's values are
                              'moved','sold','given','loaned','auctioned','donated'.</sch:assert>
                        </sch:rule>
                     </sch:pattern>
                  </constraint>
               </constraintSpec>
               <attList>
                  <attDef ident="type" mode="replace" usage="req">
                     <!-- suggested values of provenance/@type -->
                     <datatype minOccurs="1" maxOccurs="1">
                        <dataRef key="teidata.enumerated"/>
                     </datatype>
                     <valList type="semi">
                        <valItem ident="found">
                           <desc xml:lang="en" versionDate="2023-06-25"/>
                        </valItem>
                        <valItem ident="observed">
                           <desc xml:lang="en" versionDate="2023-06-25"/>
                        </valItem>
                        <valItem ident="not-observed">
                           <desc xml:lang="en" versionDate="2023-06-25"/>
                        </valItem>
                        <valItem ident="transferred">
                           <desc xml:lang="en" versionDate="2023-06-25"/>
                        </valItem>
                     </valList>
                  </attDef>
                  <attDef ident="subtype" mode="replace" usage="req">
                     <!-- suggested values of provenance/@subtype -->
                     <datatype minOccurs="1" maxOccurs="1">
                        <dataRef key="teidata.enumerated"/>
                     </datatype>
                     <valList type="semi">
                        <valItem ident="discovered">
                           <desc xml:lang="en" versionDate="2023-06-25"/>
                        </valItem>
                        <valItem ident="rediscovered">
