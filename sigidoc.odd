            <!-- idno -->
            <!-- AS 2023-11-28/OK -->
            <elementSpec ident="idno" module="header" mode="change">
               <constraintSpec ident="idno" scheme="schematron" mode="add">
                  <!-- different uses of idno/@type -->
                  <constraint>
                     <sch:pattern>
                        <sch:rule context="tei:publicationStmt/tei:idno">
                           <sch:assert test="@type = ('filename','SigiDocID','sequence')"> When idno
                              is nested inside publicationStmt, @type with values of 'filename',
                              'SigiDocID', 'sequence' must be supplied. </sch:assert>
                        </sch:rule>
                        <sch:rule context="tei:person/tei:idno">
                           <sch:assert
                              test="@type = ('PBW','PmbZ','PBE','PLP','PcBE','PLRE','PIB','Taktikon')"
                              > When idno is nested inside person, @type with values of 'PBW',
                              'PmbZ', 'PBE', 'PLP', 'PcBE', 'PLRE', 'PIB', 'Taktikon' must be
                              supplied. </sch:assert>
                        </sch:rule>
                        <sch:rule context="tei:msIdentifier/tei:idno">
                           <sch:assert test="@type = ('inv-nr-current')"> When idno is nested inside
                              msIdentifier, @type with a value of 'inv-nr-current' must be supplied.
                           </sch:assert>
                        </sch:rule>
                        <sch:rule context="tei:altIdentifier/tei:idno">
                           <sch:assert test="@type = ('inv-nr-former')"> When idno is nested inside
                              altIdentifier, @type with a value of 'inv-nr-former' must be supplied.
                           </sch:assert>
                        </sch:rule>
                        <sch:rule context="tei:rs[@type='matrix']/tei:idno">
                           <sch:assert test="@type = ('SigiDoc','PBW','Taktikon')"> When idno is
                              nested inside rs/@type='matrix', @type with values of 'SigiDoc',
                              'PBW', 'Taktikon' must be supplied. </sch:assert>
                        </sch:rule>
                     </sch:pattern>
                  </constraint>
               </constraintSpec>
               <attList>
                  <!-- suggested values of idno/@type added -->
                  <attDef ident="type" mode="change" usage="req">
                     <desc xml:lang="en" versionDate="2023-06-25">Categorizes the identifier, for
                        example as an ISBN, Social Security number, etc. Suggested values include:
                        1] ISBN; 2] ISSN; 3] DOI; 4] URI; 5] VIAF; 6] ESTC; 7] OCLC; 8] filename; 9]
                        SigiDocID; 10] sequence; 11] SigiDoc; 12] PBW; 13] PmbZ; 14] PBE; 15] PLP;
                        16] PcBE; 17] PLRE; 18] PIB; 19] Taktikon; 20] inv-nr-current; 21]
                        inv-nr-former.</desc>
                     <!-- 8 to 21 added by SigiDoc -->
                     <valList mode="change" type="semi">
                        <valItem ident="filename" mode="add">
                           <desc xml:lang="en" versionDate="2023-06-25">A name for the XML
                              file</desc>
                        </valItem>
                        <valItem ident="SigiDocID" mode="add">
                           <desc xml:lang="en" versionDate="2023-06-25">An ID given to the seal
                              edited in the XML file</desc>
                        </valItem>
                        <valItem ident="sequence" mode="add">
                           <desc xml:lang="en" versionDate="2023-06-25">A sequence of numbers to
                              navigate within a corpus</desc>
                        </valItem>
                        <valItem ident="SigiDoc" mode="add">
                           <desc xml:lang="en" versionDate="2023-06-25">An ID given to the matrix /
                              boulloterion of the seal edited in the XML file</desc>
                        </valItem>
                        <valItem ident="PBW" mode="add">
                           <desc xml:lang="en" versionDate="2023-06-25">An ID given to the
                              matrix/boulloterion or to the issuer/owner of the seal edited in the
                              XML file as established by the Prosopography of the Byzantine
                              World</desc>
                        </valItem>
                        <valItem ident="PmbZ" mode="add">
                           <desc xml:lang="en" versionDate="2023-06-25">An ID given to the
                              issuer/owner of the seal edited in the XML file as established by the
                              Prosopographie der mittelbyzantinischen Zeit Online</desc>
                        </valItem>
                        <valItem ident="PBE" mode="add">
