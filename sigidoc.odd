            <!-- rs -->
            <!-- AS 2024-03-13; last modified: AS 2024-05-28 -->
            <elementSpec ident="rs" module="core" mode="change">
               <constraintSpec ident="rs" scheme="schematron" mode="add">
                  <!-- constraining values of rs/@type when nested in collection -->
                  <constraint>
                     <sch:pattern>
                        <sch:rule context="tei:collection//tei:rs">
                           <sch:assert test="@type = 'collection-name' ">With rs nested in
                              collection, @type is required and its value is
                              'collection-name'.</sch:assert>
                        </sch:rule>
                     </sch:pattern>
                     <!-- constraining values of rs/@type when nested in layout -->
                     <sch:pattern>
                        <sch:rule context="tei:layout//tei:rs">
                           <sch:assert test="@type = ('execution','shape','matrix')">With rs nested
                              in layout, @type is required and its values are
                              'execution','shape','matrix'.</sch:assert>
                        </sch:rule>
                     </sch:pattern>
                     <sch:pattern>
                        <sch:rule context="tei:layout//tei:rs[@type='matrix']">
                           <sch:assert test="@subtype = ('surviving','not-surviving')">With rs/@type
                              nested in layout, @subtype is required and its values are
                              'surviving','not-surviving'.</sch:assert>
                        </sch:rule>
                     </sch:pattern>
                  </constraint>
               </constraintSpec>
               <attList>
                  <attDef ident="type" mode="change" usage="rec">
                     <!-- suggested values of rs/@type -->
                     <valList type="semi" mode="change">
                        <valItem ident="collection-name">
                           <desc xml:lang="en" versionDate="2024-05-28">For the name of the
                              collection.</desc>
                        </valItem>
                        <valItem ident="execution">
                           <desc xml:lang="en" versionDate="2024-02-25">For the method of production
                              of the seal.</desc>
                        </valItem>
                        <valItem ident="shape">
                           <desc xml:lang="en" versionDate="2024-02-25">For the shape of the
                              seal.</desc>
                        </valItem>
                        <valItem ident="matrix">
                           <desc xml:lang="en" versionDate="2024-02-25">For the matrix of the
                              seal.</desc>
                        </valItem>
                        <valItem ident="circumstances">
                           <desc xml:lang="en" versionDate="2024-02-26">For the circumstances of the
                              finding.</desc>
                        </valItem>
                     </valList>
                  </attDef>
                  <attDef ident="subtype" mode="change" usage="rec">
                     <!-- suggested values of rs/@subtype -->
                     <valList type="semi" mode="change">
                        <valItem ident="surviving">
                           <desc xml:lang="en" versionDate="2024-02-26">For a surviving
                              matrix.</desc>
                        </valItem>
                        <valItem ident="not-surviving">
                           <desc xml:lang="en" versionDate="2024-02-26">For a lost matrix.</desc>
                        </valItem>
                     </valList>
                  </attDef>
               </attList>
            </elementSpec>
