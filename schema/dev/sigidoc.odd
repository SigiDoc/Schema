            <!-- supportDesc --><!-- AS 2024-02-25 -->
            <elementSpec ident="supportDesc" module="msdescription" mode="change">
               <attList>
                  <attDef ident="material" mode="change" usage="opt">
                     <!-- suggested values of supportDesc/@material -->
